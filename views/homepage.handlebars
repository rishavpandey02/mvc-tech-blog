
{{#each posts as |post|}}
<div class="mx-auto m-4" style="width:66.6%">
 <a href="/post/{{id}}" style="text-decoration: none; color: inherit;">
  <div class="card">
    <div class="card-header">{{title}}</div>
    <div class="card-body">
      <h5 class="card-title">Posted By {{user.username}} on {{post.dataCreated}}</h5>
      <p class="card-text">{{content}}</p>
    </div>
  </div>
</a> 
</div>

{{/each}}
